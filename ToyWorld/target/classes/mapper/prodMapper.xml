<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.scit.toyworld.dao.ProdMapper">

<insert id="insertProd" parameterType="prod">
insert into 
	toy_product(
		prodnum
		,prodname
		,prodclass
		,prodmanufacturer
		,prodprice
		,prodstock
		,prodboxquantity
		,empid
	)values(
		#{prodNum}
		,#{prodName}
		,#{prodClass}
		,#{prodManufacturer}
		,#{prodPrice}
		,#{prodStock}
		,#{prodBoxQuantity}
		,#{empId}
	)
</insert>

</mapper>
